      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "powermon-influxdb"
          },
          "query": "from(bucket: \"power_monitoring\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"equipment_power_usage\")\n  |> filter(fn: (r) => r[\"_field\"] == \"current\")\n  |> filter(fn: (r) => r[\"machine\"] == \"${machine}\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> yield(name: \"current\")",
          "refId": "A"
        }
      ],
      "title": "Current - ${machine}",
      "type": "timeseries"
    },
